{
	"metadata": [{
		"src": [{
			"files": ["SRC/Injector/Injector.csproj"],
			"exclude": ["**/bin/**", "**/obj/**"]
		}],
		"dest": "BIN/DOC",
		"properties": {
			"TargetFramework": "netcoreapp2.2"
		}
	}],
	"build": {
		"content": [{
			"files": ["*.yml"],
			"src": "BIN/DOC"
		}],
		"dest": "DOC",
		"resource": [{
			"files": ["favicon.ico"]
		}],
		"globalMetadata": {
			"_appTitle": "Injector",
			"_appFaviconPath": "favicon.ico",
			"_disableNavbar": true,
			"_disableContribution": true
		}
	}
}