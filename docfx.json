{
	"metadata": [{
		"src": [{
			"files": ["SRC/Injector.csproj"],
			"exclude": ["**/bin/**", "**/obj/**"]
		}],
		"dest": "BIN/DOC",
		"properties": {
			"TargetFramework": "netstandard2.0"
		}
	}],
	"build": {
		"content": [{
			"files": ["*.yml"],
			"src": "BIN/DOC"
		}],
		"dest": "DOC",
		"resource": [{
			"files": ["favicon.ico"]
		}],
		"globalMetadata": {
			"_appTitle": "Injector",
			"_appFaviconPath": "favicon.ico",
			"_disableNavbar": true,
			"_disableContribution": true
		},
		"xrefService": [
			"https://xref.docs.microsoft.com/query?uid={uid}"
		]
	}
}