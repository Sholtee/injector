{
	"metadata": [{
		"src": [{
			"files": ["Injector.csproj"],
			"exclude": ["**/bin/**", "**/obj/**"]
		}],
		"dest": "../BIN/DOC",
		"properties": {
			"TargetFramework": "netstandard2.1"
		}
	}],
	"build": {
		"content": [
			{
				"files": ["*.yml"],
				"src": "../BIN/DOC"
			},
			{
				"files": ["index.md"]
			}
		],
		"dest": "../DOC",
		"resource": [{
			"files": ["icon.png"],
			"src": "../"
		}],
		"globalMetadata": {
			"_appTitle": "Injector",
			"_appFaviconPath": "icon.png",
			"_disableNavbar": true,
			"_disableContribution": true
		},
		"xrefService": [
			"https://xref.docs.microsoft.com/query?uid={uid}"
		]
	}
}